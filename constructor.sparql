PREFIX ex: <http://example.org/trends#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

CONSTRUCT {
  ?record a ex:TrendRecord ;
          ex:location ?location ;
          ex:year ?year ;
          ex:category ?category ;
          ex:rank ?rank ;
          ex:query ?query .
}
FROM <file:trends.csv#encoding=UTF-8>
WHERE {
  BIND(IRI(CONCAT("http://example.org/trends/record/", STRUUID())) AS ?record)
  BIND(STR(?location) AS ?location)
  BIND(STR(?year) AS ?year)
  BIND(STR(?category) AS ?category)
  BIND(STR(?rank) AS ?rank)
  BIND(STR(?query) AS ?query)
}